{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": { "runtime": "vercel-php@0.7.4" }
  },
  "rewrites": [
    {
      "source": "/build/(.*)",
      "destination": "/public/build/$1"
    },
    {
      "source": "/(.*\\.css)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.js)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.ico)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.svg)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.png)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.jpg)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.jpeg)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.gif)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.woff)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.woff2)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.ttf)",
      "destination": "/public/$1"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}
