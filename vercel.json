{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": { "runtime": "vercel-php@0.7.4" }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.+\\.css)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.js)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.ico)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.svg)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.png)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.jpg)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.jpeg)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.gif)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.woff)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.woff2)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.ttf)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}
